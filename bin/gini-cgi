<?php

namespace Gini {
    
    require __DIR__ . '/gini-bootstrap.php';

    class Application {

    	static function setup() {
    		CGI::setup();
    	}

    	static function main($argv) {
    		CGI::main($argv);					// 分派控制器
    	}

    	static function shutdown() {
    		CGI::shutdown();
    	}

    	static function exception($e) {
    		CGI::exception($e);
    	}

    }

}

